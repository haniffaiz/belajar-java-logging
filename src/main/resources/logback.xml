<configuration>

    <logger name="programmerzamannow" level="trace"/>



<root level="warn">
    <appender class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread%] %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>

</root>
</configuration>